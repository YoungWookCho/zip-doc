{{> header title='Zip-Doc' cssName='gallery'}}
<div class="zd-contents">
	<section class="gallery-banner-wrapper wrapper" >
		<div class="gallery-banner">
			<div class="banner-logo">
				<img src="img/img_kitchen_logo_f.png">
			</div>
			<div class="banner-text">
				집닥이 만든 <span>주방 브랜드</span><br>
				이제 그 특별함을 만나보세요!
			</div>
			<div class="banner-img">
				<img src="img/img_kitchen_banner.jpg">
			</div>
		</div>
	</section>
	<section class="gallery-navi-wrapper wrapper" >
		<ul class="gallery-navi">
			<li>
				<select id="select-space" title="space">
					<option selected="selected">-전체공간-</option>
					<option>주거공간</option>
					<option>상업공간</option>
					<option>부분시공</option>
				</select>
			</li>
			<li>
			<select id="select-space-desc" title="desc">
				<option selected="selected">-전체-</option>
			</select>
			</li>
			<li>
				<select id="select-type" title="type">
					<option selected="selected">-전체-</option>
					<option>제목</option>
					<option>태그</option>
					<option>파트너스</option>
				</select>
			</li>
			<li>
				<input class="form-control navi-input" type="text">
			</li>
			<li>
				<div class="navi-btn">검색</div>
			</li>
		</ul>
	</section>
	<section class="gallery-lists wrapper" ></section>
	<section class="gallery-pages wrapper" ></section>



</div>
{{> footer jsName='gallery'}}
